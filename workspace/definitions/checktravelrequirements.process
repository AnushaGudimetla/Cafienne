
<process id="checktravelrequirements.process" name="checktravelrequirements.process"
    implementationType="http://www.omg.org/spec/CMMN/ProcessType/Unspecified">
	<input id="destinationCountry" name="destinationCountry"/>
	<input id="travellers" name="travellers" />
	<output id="theaterDestination" name="theaterDestination" />
	<output id="travellers_requirements" name="traveller_requirements" />
	<extensionElements>
		<cafienne:implementation class="org.cafienne.cmmn.instance.process.http.HTTPCallDefinition" xmlns:cafienne="org.cafienne">
			<url>http://localhost:8380/travelrequirements</url>
			<method>POST</method>
			<post-content>{"destinationCountry": "${destinationCountry}", "travellers": "${travellers}"}</post-content>
			<http-headers>
				<http-header name="Content-Type">application/json</http-header>
			</http-headers>
		</cafienne:implementation>
	</extensionElements>
</process>

